<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devseven.gympack.setlogger">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.devseven.gympack.materialsetlogger.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WeightCalculator"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.devseven.gympack.materialsetlogger.ExerciseActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ExerciseActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PickExerciseActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CreateProgramActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LogsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ExercisesListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SettingsActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.devseven.gympack.materialsetlogger.RoutineBuilderActivity"></activity>
    </application>

</manifest>